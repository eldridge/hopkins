#!/usr/bin/perl

use strict;

use SOAP::Lite;
use YAML 'Dump';

my $job			= shift || die "usage: rpc-enqueue <task name>\n";
my $args		= { commit => 1, atomic => 0 };
my $soap		= SOAP::Lite->uri('http://localhost:8080');
my $proxy		= $soap->proxy('http://localhost:8080?session=rpc');
my $response	= $proxy->enqueue($job, $args);
my $result		= $response->result;

print Dump $result;


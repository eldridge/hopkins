#!/usr/bin/perl

use strict;

use SOAP::Lite;
use YAML 'Dump';

my $args		= { commit => 1, atomic => 0 };
my $soap		= SOAP::Lite->uri('http://localhost:8080');
my $proxy		= $soap->proxy('http://localhost:8080?session=rpc');
#my $response	= $proxy->enqueue('Test', $args);
my $response	= $proxy->status;
my $result		= $response->result;

print Dump $result;

